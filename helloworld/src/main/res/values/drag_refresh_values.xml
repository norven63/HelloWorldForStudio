<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="DragToReFreshLayout">
        <attr name="dragColumn" format="integer" />
        <attr name="dragDivider" format="reference" />
        <attr name="dragDividerHeight" format="dimension" />
        <attr name="dragHorizontalSpacing" format="dimension" />
        <attr name="dragVerticalSpacing" format="dimension" />
        <attr name="dragType">
            <enum name="listView" value="1" />
            <enum name="gridView" value="2" />
            <enum name="expandableListView" value="3" />
        </attr>
    </declare-styleable>

    <item name="dragToReFresh_headView" type="id" />
    <item name="dragToReFresh_footView" type="id" />
    <item name="firstY" type="id" />
    <color name="listSelector">#00000000</color>

</resources>